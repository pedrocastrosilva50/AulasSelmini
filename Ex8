import java.util.Scanner;

public class Ex8 {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);

        int x,y;

        System.out.print("Digite um número inteiro:");
        x = sc.nextInt();

        y = x % 10;

        System.out.print("O último digito do seu número é "+y);
    }
}
