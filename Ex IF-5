import java.util.Scanner;

public class Ex5IF {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);

        double a, b, c;

        System.out.println("Digite o primeiro lado do tri창ngulo:");
        a = sc.nextDouble();
        System.out.println("Digite o segundo lado do tri창ngulo:");
        b = sc.nextDouble();
        System.out.println("Digite o terceiro lado do tri창ngulo:");
        c = sc.nextDouble();

        if (a<b+c && b<a+c && c<a+b) {
            System.out.print("Corresponde a um tri창ngulo !");
        }
        else {
            System.out.print("Nada a ver, not triangle here.");
        }
    }
}
