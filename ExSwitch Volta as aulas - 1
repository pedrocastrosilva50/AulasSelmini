public class mingroni {
    public static void main(String[] args) {
        int valor = 0;
        if (valor == 1 || valor == 2) {
            System.out.println("A");
        }
        else if (valor == 3 || valor == 4) {
            System.out.println("B");
        }
        else if (valor == 5 || valor == 6) {
            System.out.println("C");
        }
        else {
            System.out.printf("nao pode nao man");
        }
    }
}
