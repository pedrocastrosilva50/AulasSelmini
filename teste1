public class Teste {
    public static void main(String[] args) {

        int idade, idade2;
        int livros;
        double preco;

        idade = 18;
        idade2 = 17;
        livros = 1500;
        preco = 7.50;

        System.out.println("O Pedro tem "+idade+" anos, enquanto o Renan cabaco ainda tem "+idade2+" anos");
        System.out.println("A biblioteca tem "+livros+" livros");
    }
}
